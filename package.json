{
  "name": "react-claxx",
  "version": "0.0.0-dev",
  "description": "A state management library focusing on to reduce learning cost",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "webpack-dev-server",
    "test:w": "jest --runInBand --watch",
    "test:r": "jest --runInBand --verbose",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --testMatch \"<rootDir>/src/**/*.spec.ts*\"",
    "test:cov": "npm run test:r -- --collectCoverage",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "npm run tscheck && npm run eslint && npm run test:cov",
    "prepush": "npm run test && npm run build",
    "tsclint": "tsc --noEmit --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint": "eslint src --ext ts,tsx",
    "eslint:fix": "eslint src --ext ts,tsx --fix",
    "tslint:fix": "tslint -p tsconfig.json --fix",
    "lint:w": "run-p tsclint:w eslint:w",
    "semantic-release": "semantic-release",
    "prepublishOnly": "sed -i -e 's/devDependencies/_devDependencies/g' package.json",
    "remark": "sh Taskfile remark_run",
    "remark:update": "sh Taskfile remark_run --output",
    "build": "sh Taskfile build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/claxx.git"
  },
  "keywords": [
    "react",
    "flux",
    "state-management"
  ],
  "author": "tock203 & Roman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unlight/claxx/issues"
  },
  "homepage": "https://github.com/unlight/claxx#readme",
  "dependencies": {},
  "peerDependencies": {
    "react": "16",
    "react-dom": "16"
  },
  "devDependencies": {
    "@angular/compiler": "^9.1.3",
    "@angular/compiler-cli": "^9.1.3",
    "@angular/core": "^9.1.3",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@types/deep-diff": "^1.0.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-hyperscript": "^3.0.4",
    "@types/webpack": "^4.41.12",
    "@types/webpack-dev-server": "^3.10.1",
    "@typescript-eslint/eslint-plugin-tslint": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-unicorn": "^19.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "jest-dom": "^4.0.0",
    "ng-packagr": "^9.1.1",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.5",
    "react": "^16.8.6",
    "react-dom": "^16.13.1",
    "remark": "^12.0.0",
    "remark-cli": "^8.0.0",
    "remark-license": "^5.1.1",
    "remark-toc": "^7.0.0",
    "semantic-release": "^17.0.7",
    "simplytyped": "^3.2.3",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.0",
    "tsickle": "^0.38.1",
    "tslint": "^6.1.1",
    "tslint-clean-code": "^0.2.10",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "3.8",
    "watchexec-bin": "^1.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
